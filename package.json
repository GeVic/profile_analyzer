{
  "name": "profile-analyzer",
  "version": "1.0.0",
  "description": "AI-powered CV analysis against job descriptions with React frontend and tRPC server",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd packages/server && npm run dev",
    "dev:client": "cd packages/client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cd packages/server && npm run build",
    "build:client": "cd packages/client && npm run build",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:server": "cd packages/server && npm start",
    "start:client": "cd packages/client && npm run preview",
    "clean": "npm run clean:server && npm run clean:client",
    "clean:server": "cd packages/server && npm run clean",
    "clean:client": "cd packages/client && npm run clean"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "ai",
    "cv-analysis",
    "trpc",
    "react",
    "typescript",
    "pdf-processing",
    "gemini-ai"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/profile-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/profile-analyzer/issues"
  },
  "homepage": "https://github.com/your-username/profile-analyzer#readme"
}
